{
    "type": "project",
    "require": {
        "php": "^7.2",
        "ext-json": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    }
}
